<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://schemas.org/ibanservice/fault"
	targetNamespace="http://schemas.org/ibanservice/fault"
	elementFormDefault="qualified">
	<element name="fault">
		<complexType>
			<annotation>
					<documentation>message is niet voor de gebruiker, kan ook null zijn</documentation>
				</annotation>
			<sequence>
				<element name="errorCode" type="short" nillable="false"/>

				<element name="message" type="string" minOccurs="0" maxOccurs="1" />
			</sequence>
		</complexType>
	</element> 
</schema>