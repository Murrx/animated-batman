<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions 
	name="IbanService"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:req="http://schemas.org/ibanservice/ibanrequest"
	xmlns:rsp="http://schemas.org/ibanservice/response"
	xmlns:flt="http://schemas.org/ibanservice/fault"
	xmlns:srv="http://schemas.org/ibanservice"
	targetNamespace="http://schemas.org/ibanService">
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://schemas.org.prolector/powerservice/request"
				schemaLocation="request.xsd"/>
			<xsd:import namespace="http://schemas.org.prolector/powerservice/response"
				schemaLocation="response.xsd"/>
			<xsd:import namespace="http://schemas.org.prolector/powerservice/fault"
				schemaLocation="fault.xsd"/>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="request">
		<wsdl:part name="request" element="req:request"/>
	</wsdl:message>
	<wsdl:message name="response">
		<wsdl:part name="response" element="rsp:response"/>
	</wsdl:message>
	<wsdl:message name="fault">
		<wsdl:part name="fault" element="flt:fault"/>
	</wsdl:message>
	<wsdl:portType name="WS-Interface">
		<wsdl:operation name="calculatePower">
			<wsdl:input message="srv:request"/>
			<wsdl:output message="srv:response"/>
			<wsdl:fault name="fault" message="srv:fault"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="WS-Binding" type="srv:WS-Interface">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="calculatePower">
			<soap:operation soapAction="" style="document"/>
			<wsdl:input>
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
			<wsdl:fault name="fault">
				<soap:fault name="fault" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="PowerService">
		<wsdl:port name="WS-Port" binding="srv:WS-Binding">
			<soap:address location="http://localhost:8080/ws-producer/ws-producer"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>